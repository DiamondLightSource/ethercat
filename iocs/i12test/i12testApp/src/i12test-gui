#!/bin/bash
DATADIR=$(dirname $0)
MACROS="P=''"
MACROS="$MACROS,S1=BL12I-EA-ECAT-01"
MACROS="$MACROS,S2=BL12I-EA-ECAT-02"
MACROS="$MACROS,S3=BL12I-EA-ECAT-03"
MACROS="$MACROS,S5=BL12I-EA-ECAT-05"
MACROS="$MACROS,S6=BL12I-EA-ECAT-06"
MACROS="$MACROS,S7=BL12I-EA-ECAT-07"
export EDMDATAFILES=$DATADIR
edm -x -eolc -m "$MACROS" i12test
