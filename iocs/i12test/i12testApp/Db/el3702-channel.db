# el3702-channel.db
#% macro, P, Device Prefix
#% macro, S, Device suffix
#% macro, N, Channel number
#% macro, CYCLE, Ethercat scanner channel for cycle count
#% macro, CH, Ethercat scanner channel for value
#% macro, PORT, Asyn port for ethercat scanner

record(ai, "$(P)$(S):CH$(N)")
{
  field(PINI, "YES")
  field(DTYP, "asynInt32")
  field(INP,  "@asyn($(PORT))$(CH)")
  field(SCAN, "I/O Intr")
  field(FLNK, "$(P)$(S):CYCLE$(N)")
}
record(ai, "$(P)$(S):CYCLE$(N)")
{
  field(PINI, "YES")
  field(DTYP, "asynInt32")
  field(INP,  "@asyn($(PORT))$(CYCLE)")
}

