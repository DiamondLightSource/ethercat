record(longin, "$(DEVICE):MISSED")
{
   field(DTYP, "asynInt32")
   field(SCAN, "$(SCAN)")
   field(INP,  "@asyn($(PORT))XFC1_MISSED")
}


# CAN'T use pini on asyn output records to set a default value
# because they read the initial value from the device first
record(longout, "$(DEVICE):CF:ADC1_CHANBUFF_INIT")
{
   field(OUT,  "$(DEVICE):CF:ADC1_CHANBUFF")
   field(FLNK, "$(DEVICE):CF:ADC1_CHANBUFF")
   field(DOL,  "1000000")
   field(PINI, "YES")
}

record(longout, "$(DEVICE):CF:ADC1_CHANBUFF")
{
   field(DTYP, "asynInt32")
   field(OUT,  "@asyn($(PORT))ADC1_CHANBUFF")
}

